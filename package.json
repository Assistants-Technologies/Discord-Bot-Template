{
  "name": "template",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/dev.ts",
    "watch": "nodemon --ext ts,js,mjs,cjs,json --exec yarn dev",
