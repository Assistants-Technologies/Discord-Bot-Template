  "dependencies": {
    "@discordx/importer": "^1.3.1",
    "@discordx/utilities": "^7.0.5",
    "@prisma/client": "^6.11.1",
    "discord.js": "^14.17.3",
    "discordx": "^11.12.2",
    "dotenv": "^17.0.1",
    "pg": "^8.16.3",
    "prisma": "^6.11.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.10",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "chokidar": "^4.0.3",
    "eslint": "^9.30.1",
    "prettier": "^3.4.2",
    "tsx": "^4.20.3",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=7.0.0"
